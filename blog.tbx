<attribute name="Sentiments" >-0.8;0;1;-0.6;-0.6;-0.4;-0.6;-0.4;0.4;0;0;-0.4;-0.4;-0.6;-0.6;0.6;1;-0.4;-0.4;-0.4;0.2;-0.4;-0.4;-0.6;0.4;0.4;-0.6;0.4;-0.2;-0.4;0;0.6;-0.6;-0.6</attribute>
