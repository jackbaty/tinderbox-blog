<text >// This is a collection of useful functions that add text to a designated top-level note, /Logs. These operations can be valuable for understanding complex actions.

//	Log(message): copies a message to the log
// ClearLog():		erases the log
// LogHeadline(msg): logs a message in a large font
// LogDate():	logs a message with the current date


function Log(s) {var it=&quot;/Logs&quot;;
	create(it); 
	$Text(it)=$Text(it)+s.fontSize(12)+&quot;\n&quot;;};

function ClearLog(){var it=&quot;/Logs&quot;;
	create(it); 
	$Text(it)=&quot;&quot;;};

function LogHeadline(s){var it=&quot;/Logs&quot;;
	create(it); 
	$Text(it)=$Text(it)+s.fontSize(24)+&quot;\n&quot;.fontSize(12); };

function LogDate(s){var t=date(&quot;today&quot;);
	Log(t.format(&quot;l&quot;)+&quot; &quot;+t.format(&quot;t&quot;)); };

// testing
ClearLog();
Log(&quot;Hello, world!&quot;);
LogHeadline(&quot;Wow&quot;);
Log(&quot;here we are&quot;);
LogDate();





</text>
