<attribute name="ReadCount" >16</attribute>
<attribute name="SelectionCount" >40</attribute>
<attribute name="Sentiment" >-0.2133333375</attribute>
<attribute name="Sentiments" >-0.6;0.6;-0.6;-0.6;-0.2;-0.4;-0.8;0;0.2;-0.4;0.8;-0.8;-0.2;0.6;-0.8</attribute>
