<attribute name="Name" >I don&apos;t worry too much about &quot;Lock-In&quot;</attribute>
<attribute name="SelectionCount" >9</attribute>
<attribute name="Sentiment" >-0.2285714426</attribute>
<attribute name="Sentiments" >-0.6;-0.6;-0.4;1;-0.6;-0.6;0.2</attribute>
